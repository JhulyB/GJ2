<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jhuly.wtcs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ACTIVITY.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".ACTIVITY.activity_login"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ACTIVITY.ActivityRegisterUser"
            android:label="Login"
            android:parentActivityName=".ACTIVITY.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ACTIVITY.Teste_ScrollingActivity"
            android:label="@string/title_activity_teste__scrolling"
            android:parentActivityName=".ACTIVITY.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ACTIVITY.MainActivity" />
        </activity>
        <activity
            android:name=".ACTIVITY.activity_profi_descricao"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ACTIVITY.MainActivity" />
        </activity>
        <activity
            android:name=".ACTIVITY.ActivityRegisterProfi"
            android:label="Perfil Profissional"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ACTIVITY.ScrollingActivity_register_prof"
            android:label="@string/title_activity_scrolling_activity_register_prof"
            android:parentActivityName=".ACTIVITY.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jhuly.wtcs.ACTIVITY.MainActivity" />
        </activity>
    </application>

</manifest>